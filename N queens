/* Write complete code for the required soluton */
#include <stdio.h>
#include <stdbool.h>

#define MAX 10  // Maximum size of the board

int count = 0;
bool cols[MAX], diag1[2 * MAX - 1], diag2[2 * MAX - 1];

void solve(int row, int n) {
    if (row == n) {
        count++;
        return;
    }

    for (int col = 0; col < n; col++) {
        if (!cols[col] && !diag1[row + col] && !diag2[row - col + n - 1]) {
            cols[col] = diag1[row + col] = diag2[row - col + n - 1] = true;
            solve(row + 1, n);
            cols[col] = diag1[row + col] = diag2[row - col + n - 1] = false;
        }
    }
}

int nQueen(int n) {
    count = 0;  // Reset count for each test
    for (int i = 0; i < n; i++) cols[i] = false;
    for (int i = 0; i < 2 * n - 1; i++) diag1[i] = diag2[i] = false;
    
    solve(0, n);
    return count;
}

int main() {
    int n;
    scanf("%d", &n);
    printf("%d\n", nQueen(n));
    return 0;
}

